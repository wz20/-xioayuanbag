    align-items: center;
    align-self: center;
  }
  .line{
    width: 90%;
    height: 5rpx;
    background-color: rgb(216, 216, 216);
    margin-bottom: 50rpx;
  }
  .upload{
    width: 96%;
    height: 400rpx;
    display: flex;
    align-items: center;
    align-self: center;
    justify-content: center;
    flex-direction: column;
    background-color: #fff;
    border-radius: 10rpx;
    margin-top: 20rpx;
  }
  .upload_1{
    display: flex;
    flex-direction: row;
    width: 90%;
    height: 50rpx;
  }
  
  .img{
    display: flex;
    width: 96%;
    height: 200rpx;
    background-color: #fff;
